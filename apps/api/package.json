{
  "name": "@mass-master/news-parser",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "types": "tsc --noEmit",
    "runParse": "node --loader ts-node/esm ./src/index.ts",
    "start": "node --loader ts-node/esm ./src/index.ts",
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0",
    "p-iteration": "^1.1.8",
    "telegram": "^2.26.22",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@mass-master/config": "workspace:*",
    "@mass-master/eslint-config": "workspace:*",
    "@mass-master/prettier-config": "workspace:*",
    "@mass-master/tsconfig": "workspace:*",
    "@types/node": "^20.12.12",
    "eslint": "^9.2.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "tsx": "^4.14.0",
    "typescript": "^5.4.5"
  },
  "prettier": "@mass-master/prettier-config"
}